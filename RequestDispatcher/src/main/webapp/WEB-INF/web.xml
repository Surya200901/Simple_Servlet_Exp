  <servlet-name>First Servlet</servlet-name>
  <servlet-class>com.disptacher.com.FirstServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Second Servlet</servlet-name>
  <url-pattern>/ss</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Second Servlet</servlet-name>
  <servlet-class>com.disptacher.com.SecondServlet</servlet-class>
  </servlet>
</web-app>